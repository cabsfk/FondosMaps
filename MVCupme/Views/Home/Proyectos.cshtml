<div id="map">
</div>
<div id="info-pane" class="leaflet-bar"></div>
<div id="BtnMonstrarConven">
    <button type="button" class="btn btn-primary btn-sm" id="MostrarConvenciones">
        <span class="glyphicon glyphicon-align-left" aria-hidden="true"></span> <small id="textlegend">Ocultar</small>
    </button>
</div>
<div class="col-sm-2 col-md-2" id="panel_formulario" style="max-width: 370px; min-width: 320px; ">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <center>
                <button type="button" class="btn btn-default ">
                    <span class="glyphicon glyphicon-signal"></span> @Html.ActionLink("Indicadores", "", "")

                </button>
                <button type="button" class="btn btn-default active">
                    <span class="glyphicon glyphicon-edit"></span>@Html.ActionLink("Proyectos", "Proyectos", "Home")

                </button>
            </center>
        </div>

        <div class="panel-body">
            <div>
                <div class="row">
                    <div class="col-xs-9">
                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div class="tab-pane active" id="eventos">
                                <b><center>TIPO PROYECTO</center></b>
                                <hr>
                                <div class="col-sm-12">
                                    <div class="checkbox">
                                        <label>
                                            <input type="checkbox" value="" checked id="checkFondos">
                                            <span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>
                                            FONDOS
                                        </label>
                                    </div>
                                    <div class="checkbox">
                                        <label>
                                            <input type="checkbox" value="" checked id="checkPCR">
                                            <span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>
                                            PECORS
                                        </label>
                                    </div>
                                    <div class="checkbox">
                                        <label>
                                            <input type="checkbox" value="" checked id="checkPERS">
                                            <span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>
                                            PERS
                                        </label>
                                    </div>
                                    <div>
                                        <label>Fondo: </label><br>
                                        <select class="form-control" id="SelctFondo" multiple="multiple"></select>
                                    </div>
                                </div>
                                <br>
                            </div>
                            <div class="tab-pane " id="espacios">
                                <br>
                                <form style="text-align: center;" id="formAutocompl">
                                    <h5>Ingrese el nombre del municipio o departamento de interes</h5>
                                    <hr>
                                    <div class="input-group custom-search-form">
                                        <input type="text" class="form-control" id="city" placeholder="Nombre Municipio">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button" id="BtnBusquedaMun" disabled>
                                                <span class="glyphicon glyphicon-search"></span>
                                            </button>
                                        </span>
                                        <span class="input-group-btn">
                                            <button class="btn btn-primary" type="button" id="BtnLimpiarMun">
                                                <span class="glyphicon glyphicon-trash"></span>
                                            </button>
                                        </span>
                                    </div><!-- /input-group -->
                                </form>
                                <br>
                            </div>
                            
                            <div class="tab-pane" id="tiempo">
                                <b><center>RANGO TIEMPO</center></b>
                                <hr>
                                <div class="form-group">
                                    <label>Fecha Inicio</label>
                                    <div class="input-group date">
                                        <input type="text" class=" form-control hasdatepicker" id="date_ini" data-container="body" data-toggle="popover" data-trigger="focus" data-animation="true" />
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar"></span>
                                        </span>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label>Fecha Fin</label>
                                    <div class="input-group date">
                                        <input type="text" class=" form-control hasdatepicker" id="date_fin" data-container="body" data-toggle="popover" data-trigger="focus" data-animation="true" />
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>

                    <div class="col-xs-3">
                        <!-- required for floating -->
                        <!-- Nav tabs -->
                        <ul class="nav nav-tabs tabs-right">
                            <li class="active">
                                <a href="#eventos" data-toggle="tab">
                                    <span class="glyphicon glyphicon-list-alt text-primary " id="eventosColor" aria-hidden="true" data-toggle="tooltip" data-placement="right" title="" data-original-title="Estados"></span>
                                </a>
                            </li>
                            <li >
                                <a href="#espacios" data-toggle="tab">
                                    <span class="glyphicon glyphicon-picture " id="espaciosColor" aria-hidden="true" data-toggle="tooltip" data-placement="right" title="" data-original-title="Lugar"></span>
                                </a>
                            </li>
                            <li>
                                <a href="#tiempo" data-toggle="tab">
                                    <span class="glyphicon glyphicon-time  " id="tiempoColor" aria-hidden="true" data-toggle="tooltip" data-placement="right" title="" data-original-title="Fechas"></span>
                                </a>
                            </li>
                            
                        </ul>
                    </div>
                    <br>
                    <hr>
                    
                </div>

            </div>

        </div>



    </div>
    <div class="panel-footer">
        <div>
            <button type="button" id="BuscarMapa" class="btn btn-success btn-block">
                <span class="glyphicon glyphicon-search"></span> Buscar
            </button>
        </div>
    </div>
</div>
</div>






<div id="carousel-panel-generic" style="width:230px">
    <div id="myCarousel" class="carousel slide">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
        </ol>
        <!-- Carousel items -->
        <div class="carousel-inner">
            <div class="item active">
                <div class="row-fluid">
                    <div class="col-sm-6 col-md-6" id="BaseESRIStreets" value="Streets">
                        <a href="#" class="thumbnail">
                            <img src="~/images/baselayers/calles.PNG" style="max-width:100%;">
                            <div class="carousel-caption">
                                <center>
                                    <h6>ESRI Streets</h6>
                                </center>
                            </div>
                        </a>
                    </div>
                    <div class="col-sm-6 col-md-6" id="BaseESRISatellite" value="Imagery">
                        <a href="#" class="thumbnail">
                            <img src="~/images/baselayers/satelite.PNG" style="max-width:100%;">
                            <div class="carousel-caption">
                                <center>
                                    <h6 style="color:rgba(255, 255, 255,1)">ESRI Satelite</h6>
                                </center>
                            </div>
                        </a>
                    </div>
                </div>
                <!--/row-fluid-->
            </div>
            <!--/item-->
            <div class="item">
                <div class="row-fluid">
                    <div class="col-sm-6 col-md-6" id="BaseESRITopo" value="Topographic">
                        <a href="#" class="thumbnail">
                            <img src="~/images/baselayers/topografic.PNG" style="max-width:100%;">
                            <div class="carousel-caption">
                                <center>
                                    <h6>ESRI Topographic</h6>
                                </center>
                            </div>
                        </a>
                    </div>
                    <div class="col-sm-6 col-md-6" id="BaseOSM" value="OSM">
                        <a href="#" class="thumbnail">
                            <img src="~/images/baselayers/OSM.PNG" style="max-width:100%;">
                            <div class="carousel-caption">
                                <center>
                                    <h6>OSM</h6>
                                </center>
                            </div>
                        </a>
                    </div>

                </div>
                <!--/row-fluid-->
            </div>
            <!--/item-->
        </div>
        <!--/carousel-inner-->

        <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
        </a>
        <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
        </a>
    </div>
    <!--/myCarousel-->
</div>
<div class='mouse-position'>
    <b id='mousemove'><b />
</div>
@Scripts.Render("~/bundles/appMapGlobalProy")
@Scripts.Render("~/bundles/appMapProy")